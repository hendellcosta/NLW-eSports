// Components and Props

interface ButtonProps {
  title: string;
}

function Button (props: ButtonProps) {
  return (
    <button>
      {props.title}
    </button>
  )
}

function App() {
  return (
    <>
    <Button title='Button 1' />
    <Button title='Button 2' />
    <Button title='Button 3' />
    <Button title='Button 4' />
    </>
  )
}

export default App;